{
  "name": "Ultima FM",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "js/ultima.js" ]
    }
  ],
  "browser_action": {
    "default_title": "Ultima.fm",
    "default_icon" : "favicon.png",
    "default_popup": "ultima.html"
  },
  "permissions": [
    "activeTab"
  ]
}
